class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        //O(n) solution
        size_t vector_size = nums.size();
        for(int i=0 ;i < vector_size; i++)
        {
            if(nums[i] >= target)
                return i;
        }
        return vector_size;
        
    }
};
